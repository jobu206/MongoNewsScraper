<div class="container article-container">
  <h3>Read the latest on MLB</h3>
  {{#each articles}}
  <div class="card bg-light mb-3">
    <div class="card-body">
      <h5 class="card-title">{{title}}</h5>
      <a href="{{link}}" class="card-link" target="_blank">View article on MLB.com</a>
      <p class="card-text">{{intro}}...</p>
      <a data-articleId="{{_id}}" class="btn btn-primary save-btn">Save article</a>
    </div>
  </div>
  {{/each}}
</div>